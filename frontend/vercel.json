{
    "rewrites": [
      {
        "source": "/register-user-server",
        "destination": "https://presidio-rentify-backend.vercel.app/post-signup"
      },
      {
        "source": "/login-user-server",
        "destination": "https://presidio-rentify-backend.vercel.app/post-login"
      },
      {
        "source": "/register-property-server",
        "destination": "https://presidio-rentify-backend.vercel.app/api/posts/:userId"
      },
      {
        "source": "/update-property-server",
        "destination": "https://presidio-rentify-backend.vercel.app/server-create-post"
      },
      {
        "source": "/api/get-properties",
        "destination": "https://presidio-rentify-backend.vercel.app/update-post"
      },
      {
        "source": "/delete-property-server/:propertyId",
        "destination": "https://presidio-rentify-backend.vercel.app/api/delete-post/:propertyId"
      }
    ]
  }